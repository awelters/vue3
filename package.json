    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^7.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
