{
  "name": "vue3-pinia-typescript-vite-jest-cypress",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Andrew Welters",
    "email": "awelters@gmail.com"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "dev:https": "vite --https",
    "dev:type-check": "vue-tsc --noEmit --watch",
    "lint": "eslint . --ext vue,ts,js --max-warnings 0 --fix",
    "preview": "vite preview",
    "dev:test:unit": "vitest --coverage",
    "test:unit": "vitest run --coverage",
    "test:e2e:local": "cypress open",
    "test:e2e": "cypress run --browser chrome"
  },
  "dependencies": {
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "0.1.3",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.5",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.4",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.13"
  }
}
